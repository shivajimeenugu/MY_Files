import json

# Assuming the JSON data is stored in a variable called `data`
data = {
    "@type": "QueryResult",
    "result": [
        # Add the JSON data here...
    ],
    "numberOfResults": 14
}

# Retrieve all componentIds using a list comprehension
component_ids = [component["componentId"] for component in data["result"] if "componentId" in component]

print(component_ids)
